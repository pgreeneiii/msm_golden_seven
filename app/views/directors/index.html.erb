<div class="container">
   <h1>Directors</h1>

   <% @directors.each do |director| %>

      <div class="row">
         <div class="col-md-offset-3 col-md-6" style="text-align:center;">
            <h4><%= director.name %></h4>
            <img src="<%= director.image_url %>" style="height:300px;">
         </div>
      </div>

   <% end %>

</div>
